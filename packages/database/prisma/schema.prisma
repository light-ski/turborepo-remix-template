generator client {
  provider   = "prisma-client-js"
  // https://github.com/prisma/prisma/issues/10649
  engineType = "binary"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Deployment {
  id   Int    @id @default(autoincrement()) @map("id")
  name String @map("name")

  @@map("deployments")
}
